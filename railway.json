{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "startCommand": "cross-env NODE_ENV=production node api/server/index.js",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "env": {
      "LIBRECHAT_CONFIG_PATH": "/app/librechat.yaml"
    }
  }
}
